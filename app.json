{
  "expo": {
    "name": "Chattahooligan Hymnal",
    "description": "The Chattahooligans' stadium chants and songs",
    "slug": "app",
    "privacy": "public",
    "version": "0.0.29",
    "primaryColor": "#A3D8F7",
    "icon": "./assets/big-c-icon-ios.png",
    "orientation": "portrait",
    "sdkVersion": "32.0.0",
    "splash": {
      "backgroundColor": "#A3D8F7",
      "resizeMode": "contain",
      "image": "./assets/big-c-splash-heebo.png"
    },
    "android": {
      "package": "com.thechattahooligans.app",
      "icon": "./assets/big-c-icon-android.png",
      "versionCode": 15,
      "permissions": [],
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "bundleIdentifier": "com.thechattahooligans.app",
      "supportsTablet": true,
      "loadJSInBackgroundExperimental": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Chattahooligan Hymnal will use location to ignore notifications when you are not at the stadium."
      }
    },
    "notification": {
      "icon": "./assets/speakerphone.png",
      "color": "#A3D8F7"
    }
  }
}